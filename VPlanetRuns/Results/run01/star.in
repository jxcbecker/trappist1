sName	          star
saModules	      stellar #eqtide
dMass           0.089
dAge            5e6
dRadGyra    0.5        # Radius of gyration (moment of inertia constant)


# EQTIDE Parameters
#dTidalQ        1.0e6        # Tidal phase lag
#dK2        0.5        # Love number of degree 2
#dRotPeriod      -3.0                  # Rotation period, negative -> days
#sTideModel    p2        # Tidal model, p2=CPL, t8=CTL
#saTidePerts            # Body name(s) of tidal perturbers

sStellarModel   baraffe
dSatXUVFrac     0.00037
dXUVBeta         1.16               # XUV decay power law exponent
dSatXUVTime       -6.85                # XUV Saturation timescale, negative-> in Gyr
sMagBrakingModel  matt                # Magnetic braking model

saOutputOrder 	Time -Luminosity -LXUVTot Temperature HZLimRecVenus HZLimRunaway HZLimMaxGreenhouse HZLimEarlyMars
